        <Style TargetType="zui:ZGroupBox">
            <Setter Property="HeaderTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Border
                                Width="3"
                                Background="#3399FF"
                                SnapsToDevicePixels="True"
                                UseLayoutRounding="True" />
                            <TextBlock
                                Grid.Column="1"
                                Margin="7,0"
                                Foreground="White"
                                Text="{Binding Header, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type zui:ZGroupBox}}}" />
                        </Grid>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </UserControl.Resources>
    <Grid Visibility="{Binding APP_MODE, Converter={StaticResource appModeVisableUserConv}}">
        <zui:ZTabControl
            x:Name="Tab"
            Background="#252526"
            Foreground="Black"
            Type="Card">

            <!--#region Tab_图像-->
            <TabItem>
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Path
                            Width="12"
                            Data="{StaticResource Image}"
                            Fill="#ffffff"
                            Stretch="Uniform" />
                        <TextBlock
                            Margin="5,0,0,0"
                            Foreground="#ffffff"
                            Text="图像" />
                    </StackPanel>
                </TabItem.Header>

                <zui:Accordion FontSize="16">
                    <!--#region 图像采集-->
                    <zui:AccordionItem Header="图像采集" IsSelected="True">
                        <zui:AccordionItem.Content>
                            <Grid Margin="5">
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <zui:ZGroupBox
                                    Grid.Row="0"
                                    Margin="0,0,0,5"
                                    Padding="10,10,0,0"
                                    BorderThickness="0"
                                    Header="外部导入："
                                    HeaderBackground="Transparent">
                                    <StackPanel Orientation="Horizontal">
                                        <zui:IconTextBox
                                            Width="150"
                                            Height="24"
                                            HorizontalAlignment="Left"
                                            FontSize="12"
                                            IsShowIcon="False"
                                            Watermark="图像路径..." />
                                        <zui:FlatButton
                                            Width="70"
                                            Height="24"
                                            Margin="10,0,0,0"
                                            Background="#0080FF"
                                            Content="选择文件"
                                            CornerRadius="2"
                                            FontSize="12"
                                            MouseOverBackground1="#007ACC" />
                                    </StackPanel>
                                </zui:ZGroupBox>
                                <zui:ZGroupBox
                                    Grid.Row="1"
                                    Margin="0,0,0,5"
                                    Padding="10,10,0,0"
                                    BorderThickness="0"
                                    Header="采集当前帧："
                                    HeaderBackground="Transparent">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition />
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="auto" />
                                            <ColumnDefinition Width="auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <zui:FlatCheckBox
                                            x:Name="FCB_IsDelay"
                                            Grid.Column="0"
                                            Height="24"
                                            CheckedColor="#007ACC"
                                            Content="启用延时"
                                            FontSize="14"
                                            Foreground="White"
                                            Skins="DefaultSquare" />
                                        <zui:IconTextBox
                                            Grid.Column="1"
                                            Width="60"
                                            Height="24"
                                            Margin="10,0,0,0"
                                            FontSize="12"
                                            IsEnabled="{Binding ElementName=FCB_IsDelay, Path=IsChecked}"
                                            IsShowIcon="False"
                                            Watermark="毫秒..." />
                                        <zui:FlatButton
                                            Grid.Column="2"
                                            Width="70"
                                            Height="24"
                                            Background="#0080FF"
                                            Content="采集"
                                            CornerRadius="2"
                                            FontSize="12"
                                            MouseOverBackground1="#007ACC" />
                                    </Grid>
                                </zui:ZGroupBox>
                                <zui:ZGroupBox
                                    Grid.Row="2"
                                    Margin="0,0,0,5"
                                    Padding="10,10,0,0"
                                    BorderThickness="0"
                                    Header="图像信息："
                                    HeaderBackground="Transparent">
                                    <DataGrid
                                        Width="240"
                                        HorizontalAlignment="Left"
                                        AlternationCount="2"
                                        AutoGenerateColumns="False"
                                        Background="#0969C9"
                                        BorderThickness="0"
                                        CanUserAddRows="False"
                                        CanUserSortColumns="False"
                                        CellStyle="{StaticResource DataGridCellStyle1}"
                                        ColumnHeaderStyle="{StaticResource DataGridColumnHeaderStyle1}"
                                        FontSize="14"
                                        HeadersVisibility="Column"
                                        ItemsSource="{Binding FAImageInfos}"
                                        RowStyle="{StaticResource DataGridRowStyle1}">
                                        <DataGrid.Columns>
                                            <DataGridTextColumn
                                                Width="100"
                                                Binding="{Binding PropertyName}"
                                                Header="属性"
                                                IsReadOnly="True" />
                                            <DataGridTextColumn
                                                Width="*"
                                                Binding="{Binding PropertyValue}"
                                                Header="值"
                                                IsReadOnly="True" />
                                        </DataGrid.Columns>
                                    </DataGrid>
                                </zui:ZGroupBox>
                            </Grid>
                        </zui:AccordionItem.Content>
                    </zui:AccordionItem>
                    <!--#endregion-->

                    <!--#region 图像导出-->
                    <zui:AccordionItem Header="图像导出">
                        <zui:AccordionItem.Content>
                            <Grid Margin="5">
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <zui:ZGroupBox
                                    Grid.Row="0"
                                    Margin="0,0,0,5"
                                    Padding="10,10,0,0"
                                    BorderThickness="0"
                                    Header="导出设置："
                                    HeaderBackground="Transparent">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition />
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition />
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>
                                        <UniformGrid
                                            Grid.Row="0"
                                            Grid.ColumnSpan="2"
                                            Width="{Binding RelativeSource={RelativeSource AncestorLevel=1, AncestorType=Grid, Mode=FindAncestor}, Path=ActualWidth}"
                                            Columns="2"
                                            Rows="3">
                                            <TextBlock
                                                FontSize="14"
                                                Foreground="White"
                                                Text="导出格式" />
                                            <zui:ZComboBox VerticalAlignment="Center">
                                                <ComboBoxItem IsSelected="True">BMP</ComboBoxItem>
                                                <ComboBoxItem>PNG</ComboBoxItem>
                                                <ComboBoxItem>JPEG</ComboBoxItem>
                                            </zui:ZComboBox>
                                            <TextBlock
                                                Margin="0,2"
                                                FontSize="14"
                                                Foreground="White"
                                                Text="位深度" />
                                            <zui:ZComboBox Margin="0,2">
                                                <ComboBoxItem IsSelected="True">32</ComboBoxItem>
                                                <ComboBoxItem>24</ComboBoxItem>
                                                <ComboBoxItem>8</ComboBoxItem>
                                            </zui:ZComboBox>
                                            <TextBlock
                                                Margin="0,2"
                                                FontSize="14"
                                                Foreground="White"
                                                Text="质量" />
                                            <zui:ZComboBox Margin="0,2">
                                                <ComboBoxItem IsSelected="True">无损</ComboBoxItem>
                                                <ComboBoxItem>90%</ComboBoxItem>
                                                <ComboBoxItem>80%</ComboBoxItem>
                                                <ComboBoxItem>最大压缩</ComboBoxItem>
                                            </zui:ZComboBox>
                                        </UniformGrid>
                                        <TextBlock
                                            Grid.Row="1"
                                            Grid.Column="0"
                                            Margin="0,5,0,0"
                                            FontSize="14"
                                            Foreground="White"
                                            Text="t_bmp_32_100" />
                                        <zui:FlatButton
                                            Grid.Row="1"
                                            Grid.Column="1"
                                            Width="80"
                                            Height="24"
                                            Margin="0,5,0,0"
                                            HorizontalAlignment="Right"
                                            Background="#0080FF"
                                            Content="导出到..."
                                            CornerRadius="2"
                                            FontSize="12"
                                            MouseOverBackground1="#007ACC" />
                                    </Grid>
                                </zui:ZGroupBox>
                            </Grid>
                        </zui:AccordionItem.Content>
                    </zui:AccordionItem>
                    <!--#endregion-->

                    <!--#region 图像编辑-->
                    <zui:AccordionItem Header="图像编辑">
                        <zui:AccordionItem.Content>
                            <Grid Margin="5">
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <!--#region ROI设置-->
                                <zui:ZGroupBox
                                    Grid.Row="0"
                                    Margin="0,0,0,5"
                                    Padding="10,10,0,0"
                                    BorderThickness="0"
                                    Header="ROI设置："
                                    HeaderBackground="Transparent">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition />
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>
                                        <UniformGrid
                                            Grid.Row="0"
                                            Grid.ColumnSpan="2"
                                            Width="{Binding RelativeSource={RelativeSource AncestorLevel=1, AncestorType=Grid, Mode=FindAncestor}, Path=ActualWidth}"
                                            Columns="4"
                                            Rows="3">
                                            <TextBlock
                                                VerticalAlignment="Center"
                                                FontSize="14"
                                                Foreground="White"
                                                Text="X坐标" />
                                            <zui:IconTextBox
                                                Width="50"
                                                Height="24"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Background="Transparent"
                                                BorderThickness="0,0,0,1"
                                                CaretBrush="White"
                                                FontSize="12"
                                                Foreground="White"
                                                IsShowIcon="False"
                                                Text="0" />
                                            <TextBlock
                                                VerticalAlignment="Center"
                                                FontSize="14"
                                                Foreground="White"
                                                Text="Y坐标" />
                                            <zui:IconTextBox
                                                Width="50"
                                                Height="24"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Background="Transparent"
                                                BorderThickness="0,0,0,1"
                                                CaretBrush="White"
                                                FontSize="12"
                                                Foreground="White"
                                                IsShowIcon="False"
                                                Text="0" />
                                            <TextBlock
                                                VerticalAlignment="Center"
                                                FontSize="14"
                                                Foreground="White"
                                                Text="长度" />
                                            <zui:IconTextBox
                                                Width="50"
                                                Height="24"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Background="Transparent"
                                                BorderThickness="0,0,0,1"
                                                CaretBrush="White"
                                                FontSize="12"
                                                Foreground="White"
                                                IsShowIcon="False"
                                                Text="0" />
                                            <TextBlock
                                                VerticalAlignment="Center"
                                                FontSize="14"
                                                Foreground="White"
                                                Text="宽度" />
                                            <zui:IconTextBox
                                                Width="50"
                                                Height="24"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Background="Transparent"
                                                BorderThickness="0,0,0,1"
                                                CaretBrush="White"
                                                FontSize="12"
                                                Foreground="White"
                                                IsShowIcon="False"
                                                Text="0" />
                                            <TextBlock
                                                VerticalAlignment="Center"
                                                FontSize="14"
                                                Foreground="White"
                                                Text="椭圆长轴" />
                                            <zui:IconTextBox
                                                Width="50"
                                                Height="24"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Background="Transparent"
                                                BorderThickness="0,0,0,1"
                                                CaretBrush="White"
                                                FontSize="12"
                                                Foreground="White"
                                                IsShowIcon="False"
                                                Text="0" />
                                            <TextBlock
                                                VerticalAlignment="Center"
                                                FontSize="14"
                                                Foreground="White"
                                                Text="椭圆短轴" />
                                            <zui:IconTextBox
                                                Width="50"
                                                Height="24"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Background="Transparent"
                                                BorderThickness="0,0,0,1"
                                                CaretBrush="White"
                                                FontSize="12"
                                                Foreground="White"
                                                IsShowIcon="False"
                                                Text="0" />
                                        </UniformGrid>
                                        <TextBlock
                                            Grid.Row="1"
                                            Grid.Column="0"
                                            Margin="0,5,0,0"
                                            VerticalAlignment="Center"
                                            FontSize="14"
                                            Foreground="White"
                                            Text="ROI截取类型选择" />
                                        <zui:ZComboBox
                                            Grid.Row="1"
                                            Grid.Column="1"
                                            Width="100"
                                            Margin="0,5,0,0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center">
                                            <ComboBoxItem IsSelected="True">矩形</ComboBoxItem>
                                            <ComboBoxItem>椭圆</ComboBoxItem>
                                            <ComboBoxItem>标准圆</ComboBoxItem>
                                        </zui:ZComboBox>
                                        <zui:FlatCheckBox
                                            Grid.Row="2"
                                            Grid.Column="0"
                                            Height="24"
                                            Margin="0,5,0,0"
                                            CheckedColor="#007ACC"
                                            Content="启用鼠标选取"
                                            FontSize="14"
                                            Foreground="White"
                                            Skins="DefaultSquare" />
                                        <zui:FlatButton
                                            Grid.Row="2"
                                            Grid.Column="1"
                                            Width="100"
                                            Height="24"
                                            Margin="0,5,0,0"
                                            HorizontalAlignment="Center"
                                            Background="#0080FF"
                                            Content="截取ROI"
                                            CornerRadius="2"
                                            FontSize="12"
                                            MouseOverBackground1="#007ACC" />
                                    </Grid>
                                </zui:ZGroupBox>
                                <!--#endregion-->
                                <!--#region 图像基本信息设置-->
                                <zui:ZGroupBox
                                    Grid.Row="1"
                                    Margin="0,0,0,5"
                                    Padding="10,10,0,0"
                                    BorderThickness="0"
                                    Header="基础参数设置："
                                    HeaderBackground="Transparent">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="50" />
                                            <ColumnDefinition />
                                            <ColumnDefinition Width="40" />
                                        </Grid.ColumnDefinitions>
                                        <TextBlock
                                            Grid.Row="0"
                                            Grid.Column="0"
                                            Margin="0,5,0,0"
                                            FontSize="14"
                                            Foreground="White"
                                            Text="亮度" />
                                        <zui:FlatSilder
                                            x:Name="Silder_Brightness"
                                            Grid.Row="0"
                                            Grid.Column="1"
                                            Margin="0,5,0,0"
                                            AutoToolTipPlacement="TopLeft"
                                            Interval="1"
                                            IsMoveToPointEnabled="True"
                                            IsSnapToTickEnabled="True"
                                            Maximum="100"
                                            TickFrequency="1"
                                            Value="50" />
                                        <zui:IconTextBox
                                            Grid.Row="0"
                                            Grid.Column="2"
